    }`}\r\n              alt={id}\r\n            />\r\n          ))}\r\n        </div>\r\n        <div\r\n          className=\"Controls-Miniature-Image\"\r\n          onClick={() => {\r\n            HandleScrollLeft();\r\n          }}\r\n          aria-hidden=\"true\"\r\n        >\r\n          <FaArrowDown />\r\n        </div>\r\n      </div>\r\n      <div className=\"Image-Viewer\">\r\n        <img src={images[imgPos].image} alt={imgPos} />\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\gabri\\OneDrive\\Documentos\\www\\Binario Commerce\\frontend-web\\src\\store.js",[],"C:\\Users\\gabri\\OneDrive\\Documentos\\www\\Binario Commerce\\frontend-web\\src\\data.js",[],"C:\\Users\\gabri\\OneDrive\\Documentos\\www\\Binario Commerce\\frontend-web\\src\\Reducers\\Product\\index.js",[],"C:\\Users\\gabri\\OneDrive\\Documentos\\www\\Binario Commerce\\frontend-web\\src\\Constants\\Product\\index.js",[],"C:\\Users\\gabri\\OneDrive\\Documentos\\www\\Binario Commerce\\frontend-web\\src\\Actions\\Product\\index.js",[],["83","84","85","86","87"],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","severity":1,"message":"99","line":12,"column":20,"nodeType":"100","messageId":"101","endLine":12,"endColumn":25},{"ruleId":"98","severity":1,"message":"99","line":23,"column":20,"nodeType":"100","messageId":"101","endLine":23,"endColumn":25},{"ruleId":"98","severity":1,"message":"102","line":1,"column":17,"nodeType":"100","messageId":"101","endLine":1,"endColumn":26},{"ruleId":"98","severity":1,"message":"103","line":1,"column":28,"nodeType":"100","messageId":"101","endLine":1,"endColumn":37},{"ruleId":"98","severity":1,"message":"104","line":3,"column":10,"nodeType":"100","messageId":"101","endLine":3,"endColumn":18},{"ruleId":"98","severity":1,"message":"105","line":34,"column":11,"nodeType":"100","messageId":"101","endLine":34,"endColumn":12},